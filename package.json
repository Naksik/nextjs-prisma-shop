{
  "name": "nextjs-prisma-shop",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prisma:generate": "prisma generate --schema=./src/lib/prisma/schema.prisma",
    "prisma:push": "prisma db push --schema=./src/lib/prisma/schema.prisma",
    "prisma:push:resetdb": "prisma db push --force-reset --schema=./src/lib/prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema=./src/lib/prisma/schema.prisma",
    "prisma:seed": "prisma db seed --schema=./src/lib/prisma/schema.prisma"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} src/lib/prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.18.0",
    "next": "14.2.5",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20.16.0",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.3.3",
    "prisma": "^5.18.0",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
